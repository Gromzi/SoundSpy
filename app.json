{
  "expo": {
    "name": "SoundSet",
    "slug": "SoundSet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#8B1874"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#8B1874"
      },
      "package": "com.gromzi.SoundSet",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.soundspy.io",
              "pathPrefix": "/records"
            }
          ],
          "category": [ "BROWSABLE", "DEFAULT" ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "https",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fdbd1d90-f651-4282-9c9a-ae8ada5a6ae8"
      }
    }
  }
}
